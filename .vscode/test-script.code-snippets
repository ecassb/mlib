		"prefix": "test, testscript",
		"body": [
			"#include \"minunit.h\"",
			"// Include your needes headers here",
			"",
			"// Add your test function with MU_TEST(name)",
			"",
			"MU_TEST_SUITE($1) {",
			"\t// Add the tests to run here with MU_RUN_TEST(name)",
			"}",
			"",
			"int main(__attribute__ ((unused))int argc, __attribute__ ((unused))char** argv) {",
			"MU_RUN_SUITE($1);",
			"MU_REPORT();",
			"return MU_EXIT_CODE;",
